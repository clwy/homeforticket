<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f4f8ff" >

    <include
        android:id="@+id/title_layout"
        layout="@layout/head_title_layout" />

    <RelativeLayout
        android:id="@+id/head_layout"
        android:layout_width="match_parent"
        android:layout_height="63dp"
        android:layout_below="@id/title_layout"
        android:layout_marginTop="10dp"
        android:background="#ffffff" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="@string/store_setting_headimg"
            android:textColor="#414358"
            android:textSize="16dp" />

        <ImageView
            android:id="@+id/right_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:background="@drawable/me_right_arrow" />

        <ImageView
            android:id="@+id/store_headimg"
            android:layout_width="53dp"
            android:layout_height="53dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@id/right_img"
            android:background="@drawable/default_head_img" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@drawable/me_long_line" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_alignParentBottom="true"
            android:background="@drawable/me_long_line" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/store_name_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/head_layout"
        android:background="#ffffff" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="@string/store_setting_name"
            android:textColor="#414358"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/store_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:drawablePadding="12dp"
            android:drawableRight="@drawable/me_right_arrow"
            android:text="@string/store_end"
            android:textColor="#979797"
            android:textSize="16dp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_alignParentBottom="true"
            android:background="@drawable/me_long_line" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/store_info_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/store_name_layout"
        android:layout_marginTop="20dp"
        android:background="#ffffff" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:background="@drawable/me_right_arrow" />

        <TextView
            android:id="@+id/store_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:drawableLeft="@drawable/store_info"
            android:drawablePadding="12dp"
            android:gravity="center"
            android:singleLine="true"
            android:text="@string/store_info"
            android:textColor="#414358"
            android:textSize="16dp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@drawable/me_long_line" />

        <View
            android:layout_width="293dp"
            android:layout_height="0.5dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:background="@drawable/me_short_line" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/store_address_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/store_info_layout"
        android:background="#ffffff" >

        <TextView
            android:id="@+id/store_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:drawableLeft="@drawable/store_address"
            android:drawablePadding="12dp"
            android:gravity="center"
            android:singleLine="true"
            android:textColor="#414358"
            android:textSize="16dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:background="@drawable/me_right_arrow" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/store_tel_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/store_address_layout"
        android:background="#ffffff" >

        <TextView
            android:id="@+id/store_tel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:drawableLeft="@drawable/store_tel"
            android:drawablePadding="12dp"
            android:gravity="center"
            android:singleLine="true"
            android:textColor="#414358"
            android:textSize="16dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:background="@drawable/me_right_arrow" />

        <View
            android:layout_width="293dp"
            android:layout_height="0.5dp"
            android:layout_alignParentRight="true"
            android:background="@drawable/me_short_line" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_alignParentBottom="true"
            android:background="@drawable/me_long_line" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/self_product_setting_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/store_tel_layout"
        android:layout_marginTop="20dp"
        android:background="#ffffff" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="@string/self_product_setting"
            android:textColor="#414358"
            android:textSize="16dp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@drawable/me_long_line" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_alignParentBottom="true"
            android:background="@drawable/me_long_line" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/auto_confirm_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/self_product_setting_layout"
        android:background="#ffffff" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="@string/order_confirm"
            android:textColor="#414358"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/auto_confirm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:drawablePadding="12dp"
            android:drawableRight="@drawable/me_right_arrow"
            android:textColor="#979797"
            android:textSize="16dp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/auto_closed_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/auto_confirm_layout"
        android:background="#ffffff" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="@string/order_closed"
            android:textColor="#414358"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/auto_closed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:drawablePadding="12dp"
            android:drawableRight="@drawable/me_right_arrow"
            android:textColor="#979797"
            android:textSize="16dp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@drawable/me_long_line" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_alignParentBottom="true"
            android:background="@drawable/me_long_line" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/guarantee_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/auto_closed_layout"
        android:layout_marginTop="20dp"
        android:background="#ffffff" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="@string/guarantee_text"
            android:textColor="#414358"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/guarantee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:drawablePadding="12dp"
            android:drawableRight="@drawable/me_right_arrow"
            android:textColor="#979797"
            android:textSize="16dp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@drawable/me_long_line" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_alignParentBottom="true"
            android:background="@drawable/me_long_line" />
    </RelativeLayout>

</RelativeLayout>